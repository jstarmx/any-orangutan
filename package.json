{
  "name": "gallery",
  "version": "1.0.0",
  "description": "A React gallery that renders a Flickr feed, with the ability to favourite images and filter by all/favourites.",
  "author": "James Starkie",
  "license": "MIT",
  "scripts": {
    "dev": "webpack-dev-server --open --config './webpack.dev.config.js'",
    "prod": "rm -rf ./docs/* && webpack -p --config './webpack.prod.config.js'",
    "lint": "eslint ./ --ext .js --ext .jsx && stylelint ./app/styles/**/*.scss",
    "test": "yarn lint && jest --coverage && cat ./__coverage__/lcov.info | ./node_modules/coveralls/bin/coveralls.js"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "!**/docs/**",
      "!**/__coverage__/**",
      "!**.config.js"
    ],
    "coverageDirectory": "./__coverage__"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.1",
    "babel-core": "^6.24.1",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.0.0",
    "babel-plugin-lodash": "^3.2.11",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-env": "^1.5.1",
    "babel-preset-react": "^6.24.1",
    "copy-webpack-plugin": "^4.0.1",
    "coveralls": "^2.13.1",
    "css-loader": "^0.28.4",
    "date-fns": "^1.28.5",
    "enzyme": "^2.8.2",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-react": "^7.0.1",
    "extract-text-webpack-plugin": "^2.1.0",
    "import-glob-loader": "^1.1.0",
    "jest": "^20.0.4",
    "lodash": "^4.17.4",
    "mock-local-storage": "^1.0.2",
    "mock-localstorage": "^0.1.3",
    "node-sass": "^4.5.3",
    "offline-plugin": "^4.8.1",
    "path": "^0.12.7",
    "postcss-loader": "^2.0.5",
    "prop-types": "^15.5.10",
    "react": "^15.5.4",
    "react-addons-test-utils": "^15.5.1",
    "react-dom": "^15.5.4",
    "react-redux": "^5.0.5",
    "react-test-renderer": "^15.5.4",
    "redux": "^3.6.0",
    "redux-localstorage": "^0.4.1",
    "redux-mock-store": "^1.2.3",
    "redux-thunk": "^2.2.0",
    "sass-loader": "^6.0.5",
    "style-loader": "^0.18.1",
    "stylelint": "^7.10.1",
    "stylelint-config-standard": "^16.0.0",
    "stylelint-order": "^0.4.4",
    "stylelint-webpack-plugin": "^0.7.0",
    "webpack": "^2.6.1",
    "webpack-dev-server": "^2.4.5",
    "webpack-merge": "^4.1.0",
    "webpack-notifier": "^1.5.0"
  }
}
